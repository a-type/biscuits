{
  "name": "@biscuits/server",
  "scripts": {
    "dev": "env-cmd tsx watch ./src/server.ts",
    "prestart": "cd ../packages/db && pnpm migrate",
    "start": "pnpm prestart && tsx --tsconfig ./tsconfig.json ./src/server.ts",
    "start:local": "env-cmd pnpm start",
    "build": "tsc --build tsconfig.json",
    "vapid": "node ./scripts/generateVapidKeys.js"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@a-type/utils": "^1.0.5",
    "@biscuits/db": "workspace:*",
    "@biscuits/email": "workspace:*",
    "@biscuits/trpc": "workspace:*",
    "@trpc/server": "^10.10.0",
    "@verdant-web/s3-file-storage": "^1.0.0",
    "@verdant-web/server": "^1.9.2",
    "better-sqlite3": "^9.1.1",
    "body-parser": "^1.20.2",
    "cookie": "^0.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.26.3",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/body-parser": "^1.19.5",
    "@types/cookie": "^0.5.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "tsx": "^4.6.2",
    "env-cmd": "^10.1.0",
    "web-push": "^3.6.6"
  }
}
