{
  "name": "@biscuits/server",
  "scripts": {
    "dev": "env-cmd tsx watch ./src/server.ts",
    "prestart": "pnpm migrate",
    "start": "pnpm prestart && tsx --tsconfig ./tsconfig.json ./src/server.ts",
    "start:local": "env-cmd pnpm start",
    "build": "tsc --build tsconfig.json",
    "vapid": "node ./scripts/generateVapidKeys.js",
    "migrate": "dotenv biscuits-migrate"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@a-type/auth": "^0.2.1",
    "@a-type/utils": "^1.0.5",
    "@biscuits/db": "workspace:*",
    "@biscuits/email": "workspace:*",
    "@biscuits/trpc": "workspace:*",
    "@trpc/server": "^10.44.1",
    "@verdant-web/s3-file-storage": "^1.0.14",
    "@verdant-web/server": "^2.1.2",
    "@whatwg-node/server": "^0.9.22",
    "better-sqlite3": "^9.1.1",
    "body-parser": "^1.20.2",
    "cookie": "^0.6.0",
    "googleapis": "^128.0.0",
    "itty-router": "^4.0.25",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.26.3",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/body-parser": "^1.19.5",
    "@types/cookie": "^0.5.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.11.19",
    "dotenv-cli": "^7.3.0",
    "env-cmd": "^10.1.0",
    "tsx": "^4.6.2",
    "web-push": "^3.6.6"
  }
}
