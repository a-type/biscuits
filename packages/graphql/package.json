{
	"name": "@biscuits/graphql",
	"private": false,
	"version": "0.0.1",
	"exports": {
		".": {
			"production": "./dist/index.js",
			"import": "./dist/index.js",
			"node": "./dist/index.js",
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./fetch": {
			"production": "./dist/fetch.js",
			"import": "./dist/fetch.js",
			"node": "./dist/fetch.js",
			"default": "./dist/fetch.js",
			"types": "./dist/fetch.d.ts"
		},
		"./server": {
			"production": "./dist/server.js",
			"import": "./dist/server.js",
			"node": "./dist/server.js",
			"default": "./dist/server.js",
			"types": "./dist/server.d.ts"
		}
	},
	"scripts": {
		"build": "pnpm build:transpile && pnpm build:copy",
		"build:transpile": "tsc --build",
		"build:copy": "cp src/graphql-env.d.ts dist/graphql-env.d.ts && cp src/graphql-cache.d.ts dist/graphql-cache.d.ts",
		"dev": "tsc --build --watch",
		"gen": "pnpm gen:schema && pnpm gen:types",
		"gen:schema": "gql-tada generate-schema http://localhost:6124/graphql -o schema.graphql",
		"gen:types": "gql-tada generate-output"
	},
	"type": "module",
	"dependencies": {
		"@a-type/auth-fetch": "catalog:",
		"@a-type/utils": "catalog:",
		"@apollo/client": "3.12.3",
		"@biscuits/apps": "workspace:*",
		"@biscuits/error": "workspace:*",
		"@biscuits/libraries": "workspace:*",
		"graphql": "16.10.0",
		"gql.tada": "catalog:",
		"react": "catalog:"
	},
	"devDependencies": {
		"@types/react": "catalog:",
		"typescript": "catalog:"
	}
}
