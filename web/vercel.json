{
  "routes": [
    { "src": "/appPicker/", "dest": "/appPicker/", "status": 200 },
    { "src": "/refreshSession/", "dest": "/refreshSession/", "status": 200 },
    {
      "src": "/storeRefreshToken/",
      "dest": "/storeRefreshToken/",
      "status": 200
    },
    { "src": "/[^.]+", "dest": "/", "status": 200 }
  ],
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "/models/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
